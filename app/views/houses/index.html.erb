<h1 align="center">Houses For Sale</h1>
<hr>
<div class="row">
    <% @houses.each do |house| %>
        <div class="col-md-4">
            <%= link_to (image_tag house.image, width: '100%', height: '250px', class: 'rounded'), house %>
            <h5><%= number_to_currency house.price%></h5>
            <p><%= house.address %></p>
        </div>
    <% end %>
</div>
